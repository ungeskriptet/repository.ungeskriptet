<?xml version='1.0' encoding='UTF-8'?>
<addons><addon id="repository.ungeskriptet" name="Ungeskriptet's Add-on Repo" version="1.0.0" provider-name="Ungeskriptet">
        <requires>
            <import addon="xbmc.addon" version="12.0.0" />
        </requires>
        <extension point="xbmc.addon.repository" name="Ungeskriptet's Add-on Repo">
            <info compressed="false">https://raw.githubusercontent.com/ungeskriptet/repository.ungeskriptet/master/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/ungeskriptet/repository.ungeskriptet/master/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/ungeskriptet/repository.ungeskriptet/master</datadir>
            <hashes>false</hashes>
        </extension>
        <extension point="xbmc.addon.metadata">
            <summary>Download Kodi add-ons by Ungeskriptet</summary>
            <platform>all</platform>
            <assets>
        	<icon>resources/icon.jpg</icon>
    	    </assets>
    </extension>
</addon><addon id="plugin.video.weebtv" name="WeebTV" version="0.0.1~alpha1" provider-name="Ungeskriptet">
    <requires>
	<import addon="xbmc.python" version="3.0.0" optional="true" />	
    </requires>
    <extension point="xbmc.python.pluginsource" library="addon.py">
    	<provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
	<summary>Weeb.TV - Online TV</summary>
	<description>Watch polish TV online</description>
	<disclaimer>Watch popular channels on your screen, transmitting by users from all over the world.</disclaimer>
	<platform>all</platform>
	<language>en</language>
	<website>https://gitlab.com/Ungeskriptet</website>
	<assets>
        <icon>resources/icon.png</icon>
    	</assets>
    </extension>
</addon></addons>